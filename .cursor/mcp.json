{
  "mcpServers": {
    "gs-squad-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "ts-node",
        "--require",
        "tsconfig-paths/register",
        "src/cli/main-stdio.ts",
        "--engine",
        "codex",
        "--state-mode",
        "stateful"
      ]
    },
    "proxied-github": {
      "command": "npx",
      "args": [
        "-y",
        "@growthspace-engineering/gs-mcp-proxy-pii-redactor",
        "--config",
        "./.cursor/config.json",
        "--stdio-target",
        "github",
        "--group",
        "${ SUBAGENT_ROLE }"
      ],
      "workingDirectory": "."
    }
  }
}