{
  "name": "@growthspace-engineering/gs-squad-mcp",
  "version": "1.0.2",
  "description": "A TypeScript / NestJS MCP that spawns role-specialized CLI agents using stdio",
  "main": "dist/cli/main-stdio.js",
  "types": "dist/cli/main-stdio.d.ts",
  "bin": {
    "gs-squad-mcp": "dist/cli/main-stdio.js"
  },
  "scripts": {
    "build": "npm run barrels && nest build",
    "barrels": "barrelsby -c barrelsby.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:e2e": "jest --config ./jest-e2e.config.cjs --passWithNoTests",
    "start:dev": "npm run barrels && nest start --watch",
    "commit": "cz",
    "prepare": "husky",
    "release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growthspace-engineering/gs-squad-mcp.git"
  },
  "keywords": [
    "mcp",
    "multi-agent",
    "nestjs",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/growthspace-engineering/gs-squad-mcp/issues"
  },
  "homepage": "https://github.com/growthspace-engineering/gs-squad-mcp#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/core": "^10.4.20",
    "@nestjs/platform-express": "^10.4.20",
    "ejs": "^3.1.10",
    "gray-matter": "^4.0.3",
    "nest-commander": "^3.20.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/testing": "^10.4.20",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/ejs": "^3.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "barrelsby": "^2.8.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.4.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
