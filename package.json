{
  "name": "@growthspace-engineering/gs-squad-mcp",
  "version": "1.2.0",
  "description": "A TypeScript / NestJS MCP that spawns role-specialized CLI agents using stdio",
  "main": "dist/cli/main-stdio.js",
  "types": "dist/cli/main-stdio.d.ts",
  "bin": {
    "gs-squad-mcp": "dist/cli/main-stdio.js"
  },
  "scripts": {
    "build": "npm run barrels && nest build",
    "barrels": "barrelsby -c barrelsby.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "inject": "node scripts/inject-themes.cjs ./test-results/gs-squad-mcp gs-squad-mcp",
    "inject:unit": "node scripts/inject-themes.cjs ./test-results/gs-squad-mcp/unit gs-squad-mcp-unit",
    "inject:e2e": "node scripts/inject-themes.cjs ./test-results/gs-squad-mcp/e2e gs-squad-mcp-e2e",
    "pretest": "rimraf test-results",
    "test": "jest --cache --passWithNoTests",
    "posttest": "npm run inject:unit",
    "test:watch": "jest --watch",
    "pretest:cov": "rimraf test-results",
    "test:cov": "jest --coverage --cache --passWithNoTests",
    "posttest:cov": "npm run inject:unit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "rimraf test-results",
    "test:e2e": "jest --config ./jest-e2e.config.cjs --runInBand --passWithNoTests",
    "test:e2e:cov": "jest --config ./jest-e2e.config.cjs --coverage --runInBand --passWithNoTests",
    "posttest:e2e": "npm run inject:e2e",
    "posttest:e2e:cov": "npm run inject:e2e",
    "test:e2e:watch": "jest --config ./jest-e2e.config.cjs --watch --runInBand",
    "coverage:merge": "node scripts/build-combined-coverage.cjs",
    "test:unit:cov": "jest --coverage --cache --passWithNoTests --coverageDirectory ../test-results/gs-squad-mcp/unit/coverage",
    "pretest:all:cov": "rimraf test-results",
    "test:all:cov": "concurrently --success all -n unit,e2e -c blue,magenta \"npm run test:unit:cov\" \"npm run test:e2e:cov\" && npm run coverage:merge",
    "start:dev": "npm run barrels && nest start --watch",
    "commit": "cz",
    "prepare": "husky",
    "release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growthspace-engineering/gs-squad-mcp.git"
  },
  "keywords": [
    "mcp",
    "multi-agent",
    "nestjs",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/growthspace-engineering/gs-squad-mcp/issues"
  },
  "homepage": "https://github.com/growthspace-engineering/gs-squad-mcp#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/core": "^10.4.20",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/typeorm": "^10.0.0",
    "better-sqlite3": "^9.6.0",
    "ejs": "^3.1.10",
    "gray-matter": "^4.0.3",
    "nest-commander": "^3.20.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/testing": "^10.4.20",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/ejs": "^3.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "barrelsby": "^2.8.1",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-junit": "^16.0.0",
    "jest-stare": "^2.5.2",
    "lint-staged": "^16.2.6",
    "rimraf": "^5.0.5",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.4.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
